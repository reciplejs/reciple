{
  "name": "@reciple/docgen",
  "version": "10.0.1",
  "license": "LGPL-3.0-only",
  "description": "A documentation json generator and parser for reciple",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "bin": {
    "docgen": "./dist/bin/docgen.js"
  },
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsdown --config-loader unrun",
    "docs": "../docgen/dist/bin/docgen.js",
    "check": "tsc --noEmit",
    "prepack": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/reciple",
    "directory": "packages/docgen"
  },
  "dependencies": {
    "@catplvsplus/typedoc-json-parser": "^10.2.3",
    "typedoc": "^0.27.9",
    "typedoc-plugin-mdn-links": "^4.0.15",
    "typedoc-plugin-rename-defaults": "^0.7.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
