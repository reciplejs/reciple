{
  "name": "@reciple/docgen",
  "version": "10.0.1",
  "license": "LGPL-3.0-only",
  "description": "A documentation json generator and parser for reciple",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsdown --config-loader unrun",
    "check": "tsc --noEmit",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/reciple",
    "directory": "packages/docgen"
  },
  "dependencies": {
    "typedoc": "^0.28.15"
  },
  "publishConfig": {
    "access": "public"
  }
}
