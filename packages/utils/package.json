{
  "name": "@reciple/utils",
  "version": "10.0.1",
  "license": "LGPL-3.0-only",
  "description": "Utils for reciple",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./tsconfig.json": "./assets/tsconfig.json"
  },
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsdown --config-loader unrun",
    "docs": "../docgen/dist/bin/docgen.js ../../docs/utils/docs.json",
    "check": "tsc --noEmit",
    "prepack": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/reciple",
    "directory": "packages/utils"
  },
  "dependencies": {
    "@prtty/prtty": "^1.1.0",
    "pkg-types": "^2.3.0",
    "radash": "^12.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
