{
  "name": "reciple",
  "version": "10.0.30",
  "license": "LGPL-3.0-only",
  "description": "The CLI for reciple",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": "./dist/bin/reciple.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./jsx-runtime": {
      "import": "./dist/helpers/jsx-runtime.mjs",
      "types": "./dist/helpers/jsx-runtime.d.mts"
    }
  },
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsdown --config-loader unrun",
    "docs": "deno doc --json --sloppy-imports ./src/index.ts > docs.json",
    "check": "tsc --noEmit",
    "prepack": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/reciple",
    "directory": "packages/reciple"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "@dotenvx/dotenvx": "^1.52.0",
    "@prtty/print": "catalog:",
    "@prtty/prtty": "catalog:",
    "@reciple/utils": "workspace:^",
    "@sapphire/shapeshift": "^4.0.0",
    "@sapphire/snowflake": "^3.5.5",
    "commander": "^14.0.3",
    "github-slugger": "^2.0.0",
    "globby": "^16.1.1",
    "jiti": "^2.6.1",
    "micromatch": "^4.0.8",
    "nypm": "^0.6.5",
    "pkg-types": "^2.3.0",
    "ts-mixer": "^6.0.4",
    "tsdown": "catalog:"
  },
  "devDependencies": {
    "@reciple/jsx": "workspace:^",
    "@types/micromatch": "^4.0.10",
    "@types/node": "catalog:",
    "nodemon": "^3.1.14",
    "rolldown": "^1.0.0-rc.6",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@reciple/core": "workspace:^",
    "discord.js": "catalog:"
  },
  "peerDependenciesMeta": {
    "@reciple/core": {
      "optional": false
    },
    "discord.js": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
