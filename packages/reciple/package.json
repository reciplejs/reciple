{
  "name": "reciple",
  "version": "10.0.0",
  "license": "GPL-3.0-only",
  "description": "The CLI for reciple",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./dist/bin/reciple.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/helpers/jsx-runtime.js",
      "types": "./dist/helpers/jsx-runtime.d.ts"
    }
  },
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsup",
    "check": "tsc --noEmit",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/reciple",
    "directory": "packages/reciple"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@dotenvx/dotenvx": "^1.51.1",
    "@reciple/utils": "^10.0.0",
    "@sapphire/shapeshift": "^4.0.0",
    "@sapphire/snowflake": "^3.5.5",
    "bundle-require": "^5.1.0",
    "commander": "^14.0.2",
    "detect-indent": "^7.0.2",
    "github-slugger": "^2.0.0",
    "globby": "^15.0.0",
    "joycon": "^3.1.1",
    "micromatch": "^4.0.8",
    "ora": "^9.0.0",
    "semver": "^7.7.3",
    "strip-json-comments": "^5.0.3",
    "ts-mixer": "^6.0.4",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.1"
  },
  "devDependencies": {
    "@reciple/jsx": "^10.0.0",
    "@types/micromatch": "^4.0.10",
    "@types/node": "^24.10.1",
    "@types/semver": "^7.7.1",
    "nodemon": "^3.1.11",
    "prtyprnt": "^1.1.0",
    "rimraf": "^6.1.0"
  },
  "peerDependencies": {
    "@reciple/core": "^10.0.0",
    "discord.js": "^14.24.2"
  },
  "peerDependenciesMeta": {
    "@reciple/core": {
      "optional": false
    },
    "discord.js": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
