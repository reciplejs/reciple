{
  "name": "reciple",
  "version": "10.0.1-next.11",
  "license": "LGPL-3.0-only",
  "description": "The CLI for reciple",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": "./dist/bin/reciple.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./jsx-runtime": {
      "import": "./dist/helpers/jsx-runtime.mjs",
      "types": "./dist/helpers/jsx-runtime.d.mts"
    }
  },
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsdown --config-loader unrun",
    "check": "tsc --noEmit",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/reciple",
    "directory": "packages/reciple"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@dotenvx/dotenvx": "^1.51.2",
    "@prtty/print": "^1.1.0",
    "@prtty/prtty": "^1.1.0",
    "@reciple/utils": "^10.0.1-next.4",
    "@sapphire/shapeshift": "^4.0.0",
    "@sapphire/snowflake": "^3.5.5",
    "commander": "^14.0.2",
    "github-slugger": "^2.0.0",
    "globby": "^16.1.0",
    "micromatch": "^4.0.8",
    "nypm": "^0.6.2",
    "pkg-types": "^2.3.0",
    "semver": "^7.7.3",
    "ts-mixer": "^6.0.4",
    "tsdown": "^0.18.3",
    "unrun": "^0.2.21"
  },
  "devDependencies": {
    "@reciple/jsx": "^10.0.1-next.0",
    "@types/micromatch": "^4.0.10",
    "@types/node": "^24.10.4",
    "@types/semver": "^7.7.1",
    "nodemon": "^3.1.11",
    "rolldown": "^1.0.0-beta.57",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@reciple/core": "^10.0.0 || >=10.0.1-next.0",
    "discord.js": "^14.25.1"
  },
  "peerDependenciesMeta": {
    "@reciple/core": {
      "optional": false
    },
    "discord.js": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
