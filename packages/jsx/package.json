{
  "name": "@reciple/jsx",
  "version": "10.0.0",
  "license": "GPL-3.0-only",
  "description": "The JSX component wrappers of Discord.js builders",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx-runtime.js",
      "types": "./dist/jsx-runtime.d.ts"
    },
    "./interactive": {
      "import": "./dist/components/interactive/index.js",
      "types": "./dist/components/interactive/index.d.ts"
    },
    "./embed": {
      "import": "./dist/components/embed/index.js",
      "types": "./dist/components/embed/index.d.ts"
    },
    "./modals": {
      "import": "./dist/components/modals/index.js",
      "types": "./dist/components/modals/index.d.ts"
    },
    "./poll": {
      "import": "./dist/components/poll/index.js",
      "types": "./dist/components/poll/index.d.ts"
    },
    "./v1": {
      "import": "./dist/components/v1/index.js",
      "types": "./dist/components/v1/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsup",
    "check": "tsc --noEmit",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/reciple",
    "directory": "packages/jsx"
  },
  "peerDependencies": {
    "discord.js": "^14.24.2"
  },
  "peerDependenciesMeta": {
    "discord.js": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
