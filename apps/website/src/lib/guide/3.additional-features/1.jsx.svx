        <discord-button customId="my-button-3" type="success">Success</discord-button>
        <discord-button customId="my-button-4" type="destructive">Danger</discord-button>
        <discord-button customId="my-button-5" type="primary">Premium</discord-button>
        <discord-button url="https://google.com">Link</discord-button>
      </discord-action-row>
    </discord-message>
</discord-messages>

